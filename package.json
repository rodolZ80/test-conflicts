{
    "name": "aura-test-conflicts",
    "version": "7.5.1-main-kpis-uploader-13613.1",
    "description": "A tool to import Aura locale files into the Aura (bot)",
    "bin": {
        "aura-locale-importer": "bin/aura-locale-importer"
    },
    "main": "bin/aura-locale-importer",
    "engines": {
        "node": ">=14.0.0"
    },
    "scripts": {
        "build": "npm run clean && npm run compile",
        "clean": "shx rm -fR ./lib",
        "compile": "tsc",
        "lint": "tslint --project tsconfig.json --config tslint.json \"./src/**/*.ts\"",
        "lint-jenkins": "tslint --project tsconfig.json -c tslint.json 'src/**/*.ts' --format checkstyle --out ${LINT_REPORT_PATH:-lint-report.xml}",
        "prepare": "npm run build",
        "start": "node lib/aura-locale-importer-cli",
        "test": "mocha -r ts-node/register src/**/*.spec.ts --recursive",
        "coverage-jenkins": "nyc --reporter cobertura --report-dir ${COVERAGE_REPORT_PATH:-./coverage} npm run test-jenkins",
        "test-jenkins": "mocha -r ts-node/register src/**/*.spec.ts --recursive --reporter mocha-jenkins-reporter"
    },
    "dependencies": {
        "@telefonica/aura-configuration": "main-kpis-uploader-13613",
        "@telefonica/aura-logging": "main-kpis-uploader-13613",
        "@telefonica/aura-storage-file-manager": "main-kpis-uploader-13613",
        "async": "^3.2.0",
        "chalk": "^4.1.1",
        "commander": "^6.2.1",
        "i18n-strings-files": "^1.0.7",
        "inquirer": "^5.1.0",
        "json-stable-stringify": "^1.0.1",
        "mkdirp": "^1.0.4",
        "nock": "^13.2.4",
        "parse-multipart-data": "^1.3.0",
        "prompts": "^2.4.1",
        "superagent": "^5.2.2",
        "xlsx": "^0.18.5",
        "xml2js": "^0.4.23",
        "zip-lib": "^0.7.3"
    },
    "devDependencies": {
        "@types/async": "^3.2.7",
        "@types/chai": "^4.2.3",
        "@types/chai-as-promised": "^7.1.4",
        "@types/chalk": "^2.2.0",
        "@types/express": "^4.11.1",
        "@types/json-stable-stringify": "^1.0.32",
        "@types/mkdirp": "^0.5.2",
        "@types/mocha": "^5.2.7",
        "@types/node": "^14.17.5",
        "@types/prompts": "^2.0.13",
        "@types/sinon": "^7.5.0",
        "@types/superagent": "^4.1.7",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "mocha": "^9.2.2",
        "mocha-jenkins-reporter": "^0.4.5",
        "shx": "^0.3.3",
        "sinon": "^7.5.0",
        "ts-node": "^8.6.2",
        "tslint": "^5.9.1",
        "nyc": "^15.1.0",
        "typescript": "~4.3.4"
    },
    "repository": "https://github.com/Telefonica/aura-release-train-manager",
    "nyc": {
        "extension": [
            ".ts"
        ],
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "**/*.d.ts",
            "**/*mock.ts",
            "**/*.spec.ts",
            "**/test/*"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "text"
        ],
        "all": true
    },
    "keywords": [
        "telefónica",
        "aura",
        "bot",
        "smart",
        "intelligent",
        "cognitive",
        "assistant"
    ],
    "author": "Telefónica Investigación y Desarrollo <rodolfo.bordonvillar@telefonica.com>",
    "license": "UNLICENCED",
    "bugs": {
        "url": "https://github.com/Telefonica/aura-locale-importer/issues"
    },
    "homepage": "https://github.com/Telefonica/aura-locale-importer#readme",
    "publishConfig": {
        "tag": "main-kpis-uploader-13613",
        "registry": "https://npm.pkg.github.com"
    }
}
